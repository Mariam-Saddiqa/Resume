<% include partials/header.ejs%>
<div class="container" id="skills">
<h1 class="text-center display-4 mb-4 py-5">Skills</h1>
<div class="row  mt-4">
    <div class="col-sm-6 mb-4 mb-md-0" data-aos="fade-right" data-aos-offset="300"
    data-aos-easing="ease-in-sine">
      <div class="card html">
        <div class="card-body">
          <i class="fab fa-html5 fa-4x"></i>
          <h3 class="card-title text-center py-2 mb-4">HTML</h3>
          <div class="progress">
            <div class="progress-bar progress-bar-striped progress-bar-animated bg-warning" role="progressbar" aria-valuenow="75" aria-valuemin="" aria-valuemax="100" style="width: 90%">90%</div>
          </div>
          
        </div>
      </div>
    </div>
    
    <div class="col-sm-6 mb-4 mb-md-0" data-aos="fade-left"
    data-aos-offset="500"
    data-aos-duration="500">
      <div class="card css">
        <div class="card-body">
          <i class="fab fa-css3-alt fa-4x"></i>
          <h3 class="card-title text-center py-2 mb-4">CSS</h3>
          <div class="progress">
            <div class="progress-bar progress-bar-striped progress-bar-animated bg-success" role="progressbar" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 75%">75%</div>
          </div>
          
        </div>
      </div>
    </div>
  </div> 
  
  <div class="row smooth mt-5 mb-4 mb-md-0" >
    <div class="col-sm-6 mb-4 mb-md-0" data-aos="fade-right" data-aos-offset="300"
    data-aos-easing="ease-in-sine">
      <div class="card java">
        <div class="card-body">
          <i class="fab fa-js fa-4x"></i>
          <h3 class="card-title text-center py-2 mb-4">JAVASCRIPT</h3>
          <div class="progress">
            <div class="progress-bar progress-bar-striped progress-bar-animated bg-info" role="progressbar" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 75%">75%</div>
          </div>
          
        </div>
      </div>
    </div>
    <div class="xs-margin visible-xs">
    </div>
    <div class="col-sm-6 mb-4 mb-md-0" data-aos="fade-left"
    >
      <div class="card node">
        <div class="card-body align-item-center">
          <i class="fab fa-node fa-4x"></i>
          <h3 class="card-title text-center py-2 mb-4">NODE JS</h3>
          <div class="progress">
            <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 75%">50%</div>
          </div>
          
        </div>
      </div>
    </div>
  </div>
</div>
<%include partials/footer.ejs%>